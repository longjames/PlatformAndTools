<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Relation" table="relation">
		<composite-id>
		<key-property name="shouhuan_id" type="java.lang.String"></key-property>
		<key-property name="user_id" type="java.lang.String"></key-property>
	    <key-property name="power" type="java.lang.Integer"></key-property>
	    <key-property name="administor" type="java.lang.Integer"></key-property>
	    <key-property name="relation" type="java.lang.String"></key-property>
	    </composite-id>
	</class>  
</hibernate-mapping>  