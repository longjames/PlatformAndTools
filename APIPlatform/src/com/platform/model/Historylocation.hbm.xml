<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Historylocation" table="Historylocation">
		<composite-id>
		<key-property name="shouhuan_id" type="java.lang.String"></key-property>
		<key-property name="time" type="java.util.Date"></key-property>
	    <key-property name="lng" type="java.lang.Double"></key-property>
	    <key-property name="lat" type="java.lang.Double"></key-property>
	    </composite-id>
	</class>  
</hibernate-mapping>  